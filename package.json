{
  "name": "fishermn-frontend",
  "version": "1.0.0",
  "description": "FisherMN - Community-powered ice fishing app for Minnesota",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run tailwind:watch\" \"npm run serve\"",
    "tailwind:watch": "tailwindcss -i ./src/input.css -o ./public/css/tailwind.css --watch",
    "tailwind:build": "tailwindcss -i ./src/input.css -o ./public/css/tailwind.css --minify",
    "serve": "node server.js",
    "build": "npm run tailwind:build"
  },
  "keywords": [
    "fishing",
    "ice-fishing",
    "minnesota",
    "community"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "tailwindcss": "^3.4.0",
    "wrangler": "^4.53.0"
  },
  "dependencies": {
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "bcryptjs": "^3.0.3",
    "express": "^4.18.2"
  }
}
